@{
    ViewBag.Title = "Confirmación de Registro";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<div id="header" class="headerFixed" style="z-index:-1;position:relative;">

</div>
<div class="col text-center heading-section ftco-animate animated fadeInUp">
    <h2 class="mb-0 mt-3"><span class="px-4">@ViewBag.Title</span></h2>
    <span class="subheading">Registro Existoso!!!</span>
</div>
<div class="ftco-animate animated fadeInUp">
    <article class="card-body mx-auto text-center">
        <p class="text-center m-0">Gracias @ViewBag.FullName por registrarse...</p>
        <p class="text-center">Bendiciones...</p>
        @Html.ActionLink("Ingresar","Login","Account",null, htmlAttributes: new { @class = "button btn-dark alt" })
    </article>
</div>
